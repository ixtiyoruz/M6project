function  F = fundMatrix(p1,p2)
% Computes the fundamental matrix F with the 8 points algorithm

% Build W from the correspondences:

    n = size(p1,2);
    W = zeros(8,9);
    for i = 1:n
        W(i,:) = [ p2(1) * p1(1), p2(1) * p1(2), p2(1),p2(2) * p1(1), p2(2) * p1(2), p2(2),p1(1),         p1(2),     1];
    end
    
    [U,D,V] = svd(W);
        
end

